//a new square
public class Square {
    //defining the width and height of our GUI square
     int width;
     int height;

    Square()
    {
        //every time we make a new square, set the width and height to 500 by 500
        width = 500;
        height = 500;
    }
    //if code asks for the width of our GUI square
    public int getWidth()
    {
        //return the width
        return width;
    }
    //if the code asks for the height of our GUI square
    public int getHeight()
    {
        //return the height
        return height;
    }
                    
    
    
    
    
    
}
